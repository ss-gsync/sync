{
  "name": "gsync",
  "version": "0.4.0",
  "description": "Astronomical timepiece displaying planetary harmonics and alignment patterns",
  "main": "src/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "webpack serve --mode development --open",
    "dev:server": "nodemon server/index.js",
    "build": "webpack --mode production",
    "build:dashboard": "webpack --mode production --env dashboard=true",
    "serve": "serve dist",
    "test": "jest",
    "setup": "./scripts/setup.sh",
    "download-ephe": "node scripts/download-ephe.js",
    "clean": "rm -rf dist node_modules/.cache",
    "lint": "eslint src server",
    "lint:fix": "eslint src server --fix",
    "start:pm2": "pm2 start server/index.js --name tau",
    "stop:pm2": "pm2 stop tau",
    "verify": "./scripts/verify-jpl.sh",
    "poetry:install": "poetry install",
    "poetry:shell": "poetry shell"
  },
  "keywords": [
    "astronomical",
    "astronomy",
    "ephemeris",
    "timepiece",
    "quantum",
    "security",
    "tau",
    "blockchain"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jquery": "^3.6.0",
    "moment": "^2.29.4",
    "pm2": "^5.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^8.2.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.6.1",
    "mini-css-extract-plugin": "^2.7.6",
    "nodemon": "^3.0.1",
    "serve": "^14.2.0",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
